$borderRadius = 10px//记录边框的圆角
$boxShadow = 0px 0px 30px 10px rgba(0,0,0,.1)
$formInputColor=#0182ff
.processbookWrapper
    width: 100%
    max-width: 1000px
    box-shadow: $boxShadow
    .tipsCard
        border-radius: $borderRadius $borderRadius 0px 0px
        max-width: 1000px
        width: 100%
        .strong
          font-weight: 600
        .important
          color: red
    .spin
        width: 100%
        max-width: 1000px
        min-height: 300px
        background-color: white
        display: flex
        flex-flow: column nowrap
        justify-content: flex-start
        align-items: center
        .go-back
            margin-top: 10px
        .icon 
            font-size: 100px
            margin-top: 10px
            &.success
                color rgb(101,147,74)
            &.failure
                color red
        .success-text
            font-size: 20px
            margin-top: 10px
        .success-order
            margin: 10px
            font-size: 13px
            color: #666
