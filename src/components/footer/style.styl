.footer-wrapper
    width: 100%
    background-color:  #333
    display: flex
    flex-flow: row wrap
    justify-content: space-between
    align-items: center
    padding 0px 300px
    color #999
    font-size: 13px
    .footer-team
        display: flex
        justify-content: flex-start
        align-items: center
        li
          margin: 6px
    .footer-icons
         margin: 20px
         display: flex
         flex-direction: column
         justify-content: center
         align-items: center
         img 
           margin-bottom: 5px
         span
           font-size: 10px
