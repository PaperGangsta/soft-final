.footer-wrapper
    width: 100%
    background-color:  #333
    display: flex
    flex-flow: row wrap
    justify-content: space-between 
    align-items: center
    padding 0px 14vw
    padding-top: 1.5em
    color #999
    font-size: 13px
    .footer-team
        display: flex
        justify-content: flex-start
        align-items: center
        li
          margin: 6px
    .footer-icons
         margin: 20px
         display: flex
         flex-direction: column
         justify-content: center
         align-items: center
         img 
           margin-bottom: 5px
         span
           font-size: 10px

@media screen and (max-width: 767px)
    .footer-wrapper
      justify-content: center
      padding: 0
      padding-top:1.5em
      .footer-team
        justify-content: center
      #develope_team
        padding-left: 1em
@media screen and (min-width: 767px) and (max-width :1024px)
    .footer-wrapper
      padding: 3vw